{
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": 2020,
		"sourceType": "module"
	},
	"env": {
		"browser": true,
		"node": true,
		"es6": true
	},
	"rules": {
		"semi": "error",
		"no-extra-semi": "warn",
		"curly": "warn",
		"quotes": ["error", "single", { "allowTemplateLiterals": true } ],
		"eqeqeq": "error",
		"indent": ["warn", "tab", { "SwitchCase": 1 } ]
	}
}